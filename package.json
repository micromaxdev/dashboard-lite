{
  "homepage": "http://krisgodina.github.io/micromax",
  "name": "micromax-dashboard",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && yarn dev",
    "flowise": "cd flowise && yarn dev",
    "nestjs": "cd nestjs && npm start:dev",
    "nest": "cd nest-backend && npm start:dev",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "cd frontend && npm install --legacy-peer-deps && npm run build"
  },
